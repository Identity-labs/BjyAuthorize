{
    "name": "bjyoungblood/bjy-authorize",
    "description": "Zend\\Acl based firewall system for ZF2 dispatch protection",
    "type": "library",
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/bjyoungblood/BjyAuthorize",
    "keywords": [
        "zf2",
        "acl",
        "zfc-user"
    ],
    "authors": [
        {
            "name": "Ben Youngblood",
            "email": "bx.youngblood@gmail.com",
            "homepage": "http://bjyoungblood.com/",
            "role": "Developer"
        },
        {
            "name": "Marco Pivetta",
            "email": "ocramius@gmail.com",
            "homepage": "http://ocramius.github.com/",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.3",
        "laminas/laminas-permissions-acl": "~2.2",
        "laminas/laminas-mvc": "~2.2",
        "laminas/laminas-eventmanager": "~2.2",
        "laminas/laminas-servicemanager": "~2.2",
        "laminas/laminas-http": "~2.2",
        "laminas/laminas-view": "~2.2",
        "laminas/laminas-cache": "~2.2",
        "laminas/laminas-dependency-plugin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7",
        "doctrine/common": ">=2.3,<2.5-dev",
        "laminas/laminas-developer-tools": "0.*",
        "zf-commons/zfc-user": "1.*",
        "squizlabs/php_codesniffer": "1.4.*",
        "satooshi/php-coveralls": "~0.6"
    },
    "suggests": {
        "laminas/laminas-developer-tools": "if you need to see current authorization details while developing",
        "zf-commons/zfc-user": "zfc-user provides a good default setup to get started with bjyauthorize",
        "zf-commons/zfc-user-doctrine-orm": "If you want to use BjyAuthorize with Doctrine ORM",
        "zf-commons/zfc-user-doctrine-mongo-odm": "If you want to use BjyAuthorize with Doctrine ODM"
    },
    "autoload": {
        "psr-0": {
            "BjyAuthorize\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.5-dev"
        }
    },
    "abandoned": true
}
